datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator dbml {
  provider = "prisma-dbml-generator"
}

enum Role {
  ADMIN
  USER
}

model UserProfile {
  id        String  @id @default(uuid())
  userId    String  @unique
  firstName String?
  lastName  String?
  role      Role    @default(USER)
}
